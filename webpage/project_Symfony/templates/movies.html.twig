{% extends 'base.html.twig' %}

{% block title %}Welcome!{% endblock %}
{% block stylesheets %}{% endblock %}

{% block body %}

<table class="table table-dark">
<tr>
	<th>Title</th>
	<th>Year</th>
</tr>
{% for t in movie %}
	<tr>
		<td>{{ t.title }}</td>
		<td>
		{% for m in t.actors %}
    	{{ m }}
	<br>
		{% endfor %}
		</td>
		<td>{{ t.description }}</td>
	</tr>
{% endfor %}
</table>		

{% endblock %}

{% block javascripts %}{% endblock %}