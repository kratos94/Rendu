{% extends 'base.html.twig' %}

{% block title %}BngBxStdio{% endblock %}
{% block stylesheets %}{% endblock %}

{% block body %}

<table class="table table-dark">
<tr>
	<th>Title</th>
	<th>Year</th>
</tr>
{% for t in music %}
	<tr>
		<td>{{ t.title }}</td>
		<td>
		{% for m in t.author %}
    	{{ m }}
	<br>
		{% endfor %}
		</td>
		<td>{{ t.date_added }}</td>
	</tr>
{% endfor %}
</table>	

		

{% endblock %}

{% block javascripts %}{% endblock %}
